{
  "name": "sfid-ui",
  "version": "1.0.0",
  "description": "Standalone FID Control Panel",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "ospec",
    "c8": "node node_modules/c8/bin/c8.js npm test",
    "coverage": "node node_modules/c8/bin/c8.js report --reporter=text-lcov > coverage/tests.lcov",
    "builddev": "cross-env NODE_ENV=development webpack --config webpack.development.js",
    "webdev": "cross-env NODE_ENV=development webpack-dev-server --open --config webpack.development.js",
    "startdev": "cross-env NODE_ENV=development electron .",
    "build": "cross-env NODE_ENV=production webpack --config webpack.production.js",
    "web": "cross-env NODE_ENV=production webpack-dev-server --open --config webpack.production.js",
    "start": "cross-env NODE_ENV=production electron ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamlee-ARC/sfid"
  },
  "keywords": [
    "resin",
    "io",
    "balena",
    "electron",
    "js",
    "mithril"
  ],
  "dependencies": {
    "bottlejs": "^1.7.2",
    "chart.js": "^2.9.3",
    "electron": "^9.2.0",
    "electron-rebuild": "^1.11.0",
    "is-online": "^8.4.0",
    "lockfile": "^1.0.4",
    "mithril": "^2.0.4",
    "moment-timezone": "^0.5.31",
    "normalize.css": "^8.0.1",
    "socket.io-client": "^2.3.0",
    "tachyons": "^4.12.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0",
    "xel": "0.0.230"
  },
  "author": "Adam Lee, Activated Research Company",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adamlee-ARC/sfid/issues"
  },
  "homepage": "https://github.com/adamlee-ARC/sfid#readme",
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "devDependencies": {
    "c8": "^7.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "dotenv": "^8.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "ospec": "^4.1.1",
    "require-all": "^3.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2"
  }
}
